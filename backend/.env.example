# OpenAI API Key (Required for Resume Parsing & Email Gen)
OPENAI_API_KEY=sk-your_openai_key_here

# Apify API Token (Required for Job Scraping)
APIFY_API_TOKEN=your_apify_token_here

# Firebase Admin SDK (Optional for local dev, uses default creds if skipped)
# Path to your service-account-file.json
FIREBASE_SERVICE_ACCOUNT_PATH=

# Google Cloud Project ID (Required if not using service account file)
GOOGLE_CLOUD_PROJECT=your_project_id
